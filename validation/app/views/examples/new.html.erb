<h1>Validation</h1><hr>
<div>
  <%= form_for @example do |f| %>
    <p><%= f.text_field :name, placeholder: "Name" %></p>
    <p><%= f.text_field :email, placeholder: "email" %></p>
    <p><%= text_field :person, :email_confirmation, placeholder: "confirm email" %></p>
    <%= f.submit class:"submit"%>
  <% end %>
</div>